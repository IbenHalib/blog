{% extends 'VadimBlogBundle::layout.html.twig' %}

{% block page_content %}
{{ category.name }}

<ul>
    {% for article in articles %}
        <li>
            <a href="{{ path('_see', {'id': article.id}) }}">
                {{ article.title }}
            </a>
        </li>

    {% endfor %}
</ul>
{% endblock page_content %}