<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sonata.admin.post" class="Vadim\BlogBundle\Admin\ArticleAdmin">
            <tag name="sonata.admin" manager_type="orm" group="Content" label="Article"/>
            <argument />
            <argument>Vadim\BlogBundle\Entity\Article</argument>
            <argument />
            <call method="setTranslationDomain">
                <argument>VadimBlogBundle</argument>
            </call>
        </service>
    </services>

</container>